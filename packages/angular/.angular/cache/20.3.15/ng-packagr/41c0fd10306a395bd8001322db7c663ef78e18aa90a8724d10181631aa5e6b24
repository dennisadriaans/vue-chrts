    stackedGroupedSpacing = input(0.1, ...(ngDevMode ? [{ debugName: \"stackedGroupedSpacing\" }] : []));\n    click = output();\n    tooltipWrapper = viewChild('tooltipWrapper', ...(ngDevMode ? [{ debugName: \"tooltipWrapper\" }] : []));\n    hoverValues = signal(undefined, ...(ngDevMode ? [{ debugName: \"hoverValues\" }] : []));\n    Orientation = Orientation$1;\n    isLegendTop = computed(() => this.legendPosition().startsWith('top'), ...(ngDevMode ? [{ debugName: \"isLegendTop\" }] : []));\n    stackedGroupedData = computed(() => {\n        const data = this.data();\n        const categories = this.categories();\n        const stackAndGrouped = this.stackAndGrouped();\n        const xAxis = this.xAxis();\n        const spacing = this.stackedGroupedSpacing() ?? 0.1;\n        // Local helper functions matching stackedGroupedUtils.ts\n        const extractStates = (cats) => {\n            const states = new Set();\n            Object.keys(cats).forEach((key) => {\n                const match = key.match(/([A-Z][a-z]+)$/);\n                if (match)\n                    states.add(match[1]);\n            });\n            return Array.from(states);\n        };\n        const states = extractStates(categories);\n        const groupedByState = {};\n        states.forEach((state) => {\n            groupedByState[state] = Object.keys(categories).filter((key) => key.endsWith(state));\n        });\n        const colorsByState = {};\n        Object.entries(groupedByState).forEach(([state, keys]) => {\n            colorsByState[state] = keys.map((key) => {\n                const color = categories[key]?.color;\n                return (Array.isArray(color) ? color[0] : color) ?? '#ccc';\n            });\n        });\n        const bars = {};\n        Object.entries(groupedByState).forEach(([state, keys]) => {\n            bars[state] = keys.map((key) => {\n                const baseName = key.replace(state, '').toLowerCase();\n                return (d) => d[baseName + state];\n            });\n        });\n        const colorFunctions = {};\n        Object.entries(colorsByState).forEach(([state, stateColors]) => {\n            colorFunctions[state] = (_d, i) => stateColors[i] ?? '#ccc';\n        });\n        const positions = {};\n        const numStates = states.length;\n        states.forEach((state, index) => {\n            const offset = (index - (numStates - 1) / 2) * spacing;\n            positions[state] = offset;\n        });\n        // Simple flattenData logic\n        let chartData = data;\n        return {\n            states,\n            bars,\n            colorFunctions,\n            positions,\n            chartData,\n        };\n    }, ...(ngDevMode ? [{ debugName: \"stackedGroupedData\" }] : []));\n    legendAlignment = computed(() => {\n        const pos = this.legendPosition();\n        if (pos.includes('left'))\n            return 'flex-start';\n        if (pos.includes('right'))\n            return 'flex-end';\n        return 'center';\n    }, ...(ngDevMode ? [{ debugName: \"legendAlignment\" }] : []));\n    legendItems = computed(() => {\n        return Object.values(this.categories()).map((item) => ({\n            ...item,\n            color: Array.isArray(item.color) ? item.color[0] : item.color,\n        }));\n    }, ...(ngDevMode ? [{ debugName: \"legendItems\" }] : []));\n    yAccessors = computed(() => {\n        return this.yAxis().map((key) => (d) => d[key]);\n    }, ...(ngDevMode ? [{ debugName: \"yAccessors\" }] : []));\n    labelData = computed(() => {\n        const data = this.data();\n        const keys = this.yAxis();\n        return data.flatMap((item, colIndex) => keys.map((key, itemIndex) => ({\n            x: colIndex,\n            y: Number(item[key] ?? 0),\n            itemIndex,\n            datum: item,\n        })));\n    }, ...(ngDevMode ? [{ debugName: \"labelData\" }] : []));\n    labelX = computed(() => {\n        const isStacked = this.stacked();\n        const numBars = this.yAxis().length;\n        const groupPadding = this.groupPadding();\n        const barPadding = this.barPadding();\n        return (d) => {\n            // Do not offset for stacked variants\n            if (isStacked)\n                return d.x;\n            if (numBars <= 1)\n                return d.x; // single series, already centered\n            // Effective drawable width of the group after outer group padding (heuristic)\n            const groupEffectiveWidth = 1 - groupPadding;\n            const barEffectiveWidth = groupEffectiveWidth / numBars;\n            // Start (left) edge of effective group relative to group center (which is at d.x)\n            const leftOffsetFromCenter = -groupEffectiveWidth / 2;\n            const barCenterOffset = barEffectiveWidth * d.itemIndex + barEffectiveWidth / 2;\n            // Bars inside the group usually don't span the full theoretical width due to internal barPadding.\n            const compression = 1 - barPadding; // pull towards center\n            const offsetFromCenter = (leftOffsetFromCenter + barCenterOffset) * compression;\n            return d.x + offsetFromCenter;\n        };\n    }, ...(ngDevMode ? [{ debugName: \"labelX\" }] : []));\n    labelY = computed(() => {\n        const spacing = this.valueLabel()?.labelSpacing ?? 0;\n        return (d) => d.y + spacing;\n    }, ...(ngDevMode ? [{ debugName: \"labelY\" }] : []));\n    labelAccessor = computed(() => {\n        const labelFn = this.valueLabel()?.label;\n        return (d, i) => {\n            if (!labelFn)\n                return '';\n            const value = labelFn(d, i);\n            return value == null ? '' : String(value);\n        };\n    }, ...(ngDevMode ? [{ debugName: \"labelAccessor\" }] : []));\n    colorAccessor = (_, i) => {\n        const cats = Object.values(this.categories());\n        return cats[i]?.color;\n    };\n    _x = (_, i) => i;\n    getXForState(state) {\n        return (_, i) => i + this.stackedGroupedData().positions[state];\n    }\n    xFormatterFn = (tick, i, ticks) => {\n        const formatter = this.xFormatter();\n        return formatter ? formatter(tick, i, ticks) : String(tick);\n    };\n    yFormatterFn = (tick, i, ticks) => {\n        const formatter = this.yFormatter();\n        return formatter ? formatter(tick, i, ticks) : String(tick);\n    };\n    tooltipTriggers = {\n        [GroupedBar.selectors.bar]: (d) => {\n            this.onCrosshairUpdate(d);\n            return d ? this.tooltipWrapper()?.nativeElement.innerHTML ?? '' : '';\n        },\n        [StackedBar.selectors.bar]: (d) => {\n            this.onCrosshairUpdate(d);\n            return d ? this.tooltipWrapper()?.nativeElement.innerHTML ?? '' : '';\n        },\n    };\n    onCrosshairUpdate(d) {\n        this.hoverValues.set(d);\n        this.cdr.detectChanges();\n    }\n    onClick(event) {\n        this.click.emit({ event, values: this.hoverValues() });\n    }\n    cdr = inject(ChangeDetectorRef);\n    static ɵfac = i0.ɵɵngDeclareFactory({ minVersion: \"12.0.0\", version: \"20.3.16\", ngImport: i0, type: BarChartComponent, deps: [], target: i0.ɵɵFactoryTarget.Component });\n    static ɵcmp = i0.ɵɵngDeclareComponent({ minVersion: \"17.0.0\", version: \"20.3.16\", type: BarChartComponent, isStandalone: true, selector: \"bar-chart\", inputs: { data: { classPropertyName: \"data\", publicName: \"data\", isSignal: true, isRequired: true, transformFunction: null }, height: { classPropertyName: \"height\", publicName: \"height\", isSignal: true, isRequired: false, transformFunction: null }, padding: { classPropertyName: \"padding\", publicName: \"padding\", isSignal: true, isRequired: false, transformFunction: null }, categories: { classPropertyName: \"categories\", publicName: \"categories\", isSignal: true, isRequired: true, transformFunction: null }, yAxis: { classPropertyName: \"yAxis\", publicName: \"yAxis\", isSignal: true, isRequired: true, transformFunction: null }, stacked: { classPropertyName: \"stacked\