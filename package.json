{
  "version": "0.0.0",
  "name": "vue-chrts-monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build --filter=!nuxt-charts-cli",
    "dev": "turbo run dev --filter=!nuxt-charts-cli",
    "test": "turbo run test --filter=!nuxt-charts-cli",
    "lint": "turbo run lint --filter=!nuxt-charts-cli",
    "typecheck": "turbo run typecheck --filter=!nuxt-charts-cli",
    "cli:build": "pnpm --filter nuxt-charts-cli build",
    "cli:dev": "pnpm --filter nuxt-charts-cli dev",
    "cli:test": "pnpm --filter nuxt-charts-cli test",
    "vue:release": "pnpm --filter vue-chrts release",
    "nuxt:release": "pnpm --filter nuxt-charts release"
  },
  "packageManager": "pnpm@8.15.4+sha256.cea6d0bdf2de3a0549582da3983c70c92ffc577ff4410cbf190817ddc35137c2",
  "devDependencies": {
    "turbo": "^2.5.5"
  }
}
