{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".nuxt/**"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "dev": {
      "cache": false
    },
    "version": {
      "cache": false
    },
    "publish": {
      "dependsOn": ["build", "test"],
      "cache": false
    }
  }
}
